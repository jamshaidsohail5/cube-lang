use cube.lang.cst.cstTranslator
use cube.lang.cst.token
use cube.lang.parser.leftParser
use cube.lang.parser.parser
use cube.lang.cst.cstNodeType.IDENTIFIER

define identifierParser[T] as leftParser[T]
  function parse(parser as parser, token as token) as T
    var v as cstTranslator[T] = parser.translator
    v.start(IDENTIFIER)
    v.accept(token)
    output v.value
  end
end