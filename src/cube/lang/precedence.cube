enum precedence
  AssignmentPrecedence(1)
  TypePrecedence(2)
  LogicalOrPrecedence(3)
  LogicalAndPrecedence(4)
  BinaryOrPrecedence(5)
  BinaryXorPrecedence(6)
  BinaryAndPrecedence(7)
  EqualityPrecedence(8)
  RelationalPrecedence(9)
  AdditivePrecedence(10)
  MultiplicativePrecedence(11)
  UnaryPrefixPrecedence(12)
  UnaryPostfixPrecedence(13)
  FunctionPrecedence(14)
  DotPrecedence(15)

  function value as int = value

  internal
    var value as int

    function create(value as int) = me.value = value
  end
end