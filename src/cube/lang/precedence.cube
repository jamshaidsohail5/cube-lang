enum precedence
  ConditionalPrecedence(1)
  AssignmentPrecedence(2)
  LogicalOrPrecedence(3)
  LogicalAndPrecedence(4)
  BinaryOrPrecedence(5)
  BinaryXorPrecedence(6)
  BinaryAndPrecedence(7)
  RelationalPrecedence(8)
  AdditivePrecedence(9)
  MultiplicativePrecedence(10)
  UnaryPrefixPrecedence(11)
  UnaryPostfixPrecedence(12)
  FunctionPrecedence(13)
  DotPrecedence(14)

  function value as int = value

  internal
    var value as int

    function create(value as int) = me.value = value
  end
end