use cube.lang.operator

interface cstTranslator[T]
  function enter(nodeType as cstNodeType) as cstTranslator[T]
  function start(nodeType as cstNodeType) as void = start(nodeType, null)
  function start(nodeType as cstNodeType, operator as operator)
  function child(token as token)
  function child(value as T)
  function value as T
  function value(token as token) as T
end