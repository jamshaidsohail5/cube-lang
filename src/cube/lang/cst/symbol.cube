enum symbol
  LEFT_PARENTHESIS('(')
  RIGHT_PARENTHESIS(')')
  COMMA(',')
  EQUAL('=')
  PLUS('+')
  MINUS('-')
  ASTERISK('*')
  FORWARD_SLASH('/')
  CARET('^')
  TILDE('~')
  EXCLAMATION_MARK('!')
  QUESTION_MARK('?')
  COLON(':')

  function text as string = text

  internal
    var text as string

    function create(text as string) = me.text = text
  end
end