use cube.lang.element
use cube.lang.operator

define binaryExpression as element
  function create(operator as operator, left as element, right as element)
    me.operator = operator
    me.left = left
    me.right = right
  end

  function write(s as stringBuilder)
    s.append('(')
    left.write(s)
    s.append(' ').append(operator.symbol.text).append(' ')
    right.write(s)
    s.append(')')
  end

  internal
    var operator as operator
    var left as element
    var right as element
  end
end