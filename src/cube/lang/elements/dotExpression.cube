use cube.lang

define dotExpression as element
  function create(left as element, right as element)
    me.left = left
    me.right = right
  end

  function write(s as stringBuilder)
    s.append('(')
    left.write(s)
    s.append('.')
    right.write(s)
    s.append(')')
  end

  internal
    var left as element
    var right as element
  end
end