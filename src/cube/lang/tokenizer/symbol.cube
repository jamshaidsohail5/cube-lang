enum symbol
  DOT('.')
  LEFT_PARENTHESIS('(')
  RIGHT_PARENTHESIS(')')
  COMMA(',')
  EQUAL('=')
  PLUS('+')
  PLUS_PLUS('++')
  MINUS('-')
  MINUS_MINUS('--')
  ASTERISK('*')
  FORWARD_SLASH('/')
  CARET('^')
  TILDE('~')
  LESS('<')
  LEFT_ARROW('<-')
  LESS_EQUAL('<=')
  GREATER('>')
  RIGHT_ARROW('->')
  GREATER_EQUAL('>=')

  function text as string = text

  internal
    var text as string

    function create(text as string) = me.text = text
  end
end