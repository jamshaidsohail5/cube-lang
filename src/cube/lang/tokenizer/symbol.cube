enum symbol
  Dot('.')
  LeftParenthesis('(')
  RightParenthesis(')')
  Comma(',')
  Equal('=')
  Plus('+')
  PlusPlus('++')
  Minus('-')
  MinusMinus('--')
  Asterisk('*')
  ForwardSlash('/')
  Caret('^')
  Tilde('~')
  LessSymbol('<')
  LessEqual('<=')
  GreaterSymbol('>')
  GreaterEqual('>=')
  LeftArrowSymbol('<-')
  RightArrowSymbol('->')

  function text as string = text

  internal
    var text as string

    function create(text as string) = me.text = text
  end
end