enum symbol
  Dot('.')
  LeftParenthesis('(')
  RightParenthesis(')')
  LeftCurlyBracket('{')
  RightCurlyBracket('}')
  LeftSquareBracket('[')
  RightSquareBracket(']')
  Comma(',')
  EqualSymbol('=')
  EqualEqual('==')
  Plus('+')
  PlusPlus('++')
  PlusEqual('+=')
  Exclamation('!')
  ExclamationEqual('!=')
  Percent('%')
  PercentEqual('%=')
  Minus('-')
  MinusMinus('--')
  MinusEqual('-=')
  Asterisk('*')
  ForwardSlash('/')
  Caret('^')
  Tilde('~')
  LessSymbol('<')
  LessEqual('<=')
  GreaterSymbol('>')
  GreaterEqual('>=')
  LeftArrowSymbol('<-')

  function text as string = text

  internal
    var text as string

    function create(text as string) = me.text = text
  end
end