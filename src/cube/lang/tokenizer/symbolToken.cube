use cube.lang.syntax

define symbolToken as token
  function create(symbol as symbol, text as string)
    super(NodeType.Symbol, text)
    me.symbol = symbol
  end

  function key as symbol = symbol
  function isSymbol(s as symbol) = s == symbol

  function write(s as stringBuilder)
    s.append(nodeType.displayName)
    s.append("('")
    s.append(symbol.text)
    s.append("')")
  end

  internal
    var symbol as symbol
  end
end